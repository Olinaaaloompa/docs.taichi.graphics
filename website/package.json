{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn run flaturl && docusaurus start",
    "build": "yarn run flaturl && yarn run validateplugin && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "crowdin": "crowdin",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "crowdin:upload": "docusaurus write-translations && crowdin upload",
    "apiversion": "node ./apiversion.js",
    "validateplugin": "node ./scripts/validation/index.js",
    "flaturl": "rm -rf .flatdocs && cp -r docs .flatdocs && node ./scripts/flaturl"
  },
  "dependencies": {
    "@crowdin/cli": "^3.6.4",
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@headlessui/react": "^1.5.0",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^5.5.0",
    "animate.css": "^4.1.1",
    "antd": "^4.19.2",
    "bootstrap": "^5.1.3",
    "clsx": "^1.1.1",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "globby": "^11.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "react": "^17.0.2",
    "react-app-rewired": "^2.2.1",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.33.1",
    "react-responsive-carousel": "^3.2.23",
    "remark": "^13.0.0",
    "sine-waves": "^0.3.0",
    "swiper": "^8.0.7",
    "typeit-react": "^2.3.3",
    "url-loader": "^4.1.1",
    "wasm-loader": "^1.3.0",
    "webpack": "5"
